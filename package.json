{
  "name": "msb-api",
  "version": "0.1.0",
  "description": "API para Maxima Senales Binarias",
  "main": "main.js",
  "repository": "https://nevtep@bitbucket.org/nevtep/msb-api.git",
  "author": "Tom√°s Corellano",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node 'dist/server.js'",
    "debug": "node -r ts-node/register -r dotenv/config --inspect-brk 'src/server.ts' src/server.ts",
    "build": "tsc -p . && ncp src/schema dist/schema && ncp src/datasources/models dist/datasources/models && ncp src/_ssl dist/_ssl",
    "db:migrate": "cd src/datasources && NODE_TLS_REJECT_UNAUTHORIZED=0 npx sequelize-cli db:migrate",
    "db:drop": "cd src/datasources && NODE_TLS_REJECT_UNAUTHORIZED=0 npx sequelize-cli db:migrate:undo:all",
    "db:seed": "cd src/datasources && NODE_TLS_REJECT_UNAUTHORIZED=0 npx sequelize-cli db:seed:all",
    "generate:migration": "npx sequelize-cli migration:generate --config ./src/datasources/config/config.json --migrations-path ./src/datasources/migrations/ --name",
    "start:dev": "npm run build:dev",
    "build:dev": "nodemon -r dotenv/config 'src/server.ts' --exec 'ts-node' src/server.ts -e ts,graphql",
    "redis": "docker run --name redis -p 6379:6379 -d redis"
  },
  "dependencies": {
    "apollo-datasource-rest": "^0.9.3",
    "apollo-server": "^2.15.0",
    "apollo-server-express": "^2.12.0",
    "bcrypt": "^4.0.1",
    "compression": "^1.7.4",
    "connect-redis": "^4.0.4",
    "cors": "^2.8.5",
    "csv-parser": "^2.3.3",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphql": "^14.7.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import-node": "^0.0.4",
    "graphql-passport": "^0.6.3",
    "http": "^0.0.1-security",
    "isemail": "^3.2.0",
    "moment": "^2.27.0",
    "ncp": "^2.0.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.0.3",
    "pg-hstore": "^2.3.3",
    "redis": "^3.0.2",
    "sequelize": "^5.21.7",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "^3.5.30",
    "@types/compression": "^1.7.0",
    "@types/connect-redis": "^0.0.13",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/graphql": "^14.5.0",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/moment": "^2.13.0",
    "@types/node": "^13.13.4",
    "@types/passport": "^1.0.3",
    "@types/passport-facebook": "^2.1.9",
    "@types/passport-local": "^1.0.33",
    "@types/uuid": "^7.0.3",
    "@types/validator": "^13.0.0",
    "dotenv": "^8.2.0",
    "graphql-import": "^1.0.2",
    "nodemon": "^2.0.3",
    "sequelize-cli": "^5.5.1",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  }
}
